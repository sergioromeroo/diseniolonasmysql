  <div class="container-total">
  <!-- categorias carteles -->
  .
        <section class="container-products">

            <h2 class="productos-title">Letreros</h2>
            <ul class="container-stickers">
              
              <% Letreros.forEach(pr => { %>
                <li class="product-box">
                  <a href="/products/detail/<%= pr.id %>">
                    <figure class="product-box_image">
                      <img src="/images/<%= pr.images[0] ? pr.images[0].file  : null %>" alt="imagen de producto">
                    </figure>
                    <article class="product-box_data">
                      <h2><%= pr.name %></h2>
                      <p><%= pr.description %></p>
                      <i class="fas fa-truck"></i>
                    </article>
                  </a>
                </li>
                
              <%}); %>
            </ul>

        </section>

